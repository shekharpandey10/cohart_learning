generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Boss {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  age      Int?
  city     String
}

model Todo {
  id          Int    @id
  title       String
  description String
  userId      Int
}
